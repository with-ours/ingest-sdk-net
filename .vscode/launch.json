{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ExampleApp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/OursPrivacySdk/src/Com.OursPrivacy.ExampleApp/bin/Debug/net8.0/Com.OursPrivacy.ExampleApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/OursPrivacySdk/src/Com.OursPrivacy.ExampleApp",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/OursPrivacySdk/src/Com.OursPrivacy.ExampleApp/Views"
            }
        }
    ]
}